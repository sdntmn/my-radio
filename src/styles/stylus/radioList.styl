.radioList
  margin: 4px 0 10px;
  max-height: calc(100vh - 100px - 100px - 14px);
  width: 100%;
  border-radius: 10px;
  box-shadow: inset 0 -3em 3em rgba(0, 0, 0, 0.2), 0 0 0 1px #00adb08d,
    0.3em 0.3em 1em rgba(0, 0, 0, 0.3);

  grid-area: sbLeft;


  &__title
    margin: 0 auto;
    padding: 0;
    text-align: center;
    width: 100%;
    box-shadow: inset 0 -1em 1em rgba(0, 0, 0, 0.2), 0 0 0 1px #00adb08d,
    0.1em 0.1em 0.1em rgba(0, 0, 0, 0.3);
    border-radius: 3px;
    color: aquamarine;

  &__items
    margin: 0;
    padding: 0 0 20px;
    list-style: none;
    overflow-y: auto;
    max-height: 95%;
    max-width: 100%;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    gap: .4vh;





